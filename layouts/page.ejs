<div class="page">
    <div class="header"></div>
    <div class="container">
        <h1 class="page-title"><%- title%></h1>
        <div class="main">
            <div class="left">
                <% if (typeof(banner) !== 'undefined' && banner !== null) {%>
                <div class="page-banner">
                    <img src="<%- banner %>"/>
                </div>
                <% } %>
                <div class="content">
                    <%- contents %>
                </div>
            </div>
            <div class="right">
                <%- include('partials/service_list') %>
            </div>
        </div>

    </div>
</div>
