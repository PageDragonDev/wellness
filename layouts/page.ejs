<div class="page">
    <div class="header">
        <% if (typeof(banner) !== 'undefined' && banner !== null) {%>
            <img class="banner" src="<%- banner %>" alt="">
        <% } %>
    </div>
    <div class="container">
        <h1 class="page-title"><%- title%></h1>
        <div class="main">
            <div class="left">
                <div class="content">
                    <%- contents %>
                </div>
            </div>
        </div>

    </div>
</div>
